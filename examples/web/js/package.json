{
  "name": "shenai-sdk-example-js",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "serve": "^13.0.2",
    "shenai-sdk": "file:./shenai-sdk"
  },
  "scripts": {
    "build": "yarn upgrade shenai-sdk && rm -rf build && mkdir build && cp -r src/* node_modules/shenai-sdk build/",
    "serve": "serve -s build --config=../serve.json",
    "start": "yarn build && yarn serve"
  }
}